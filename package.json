{
  "name": "@artian-techs/adonis-breadcrumbs",
  "version": "1.0.0-0",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "type": "module",
  "license": "MIT",
  "files": [
    "build",
    "!build/bin",
    "!build/tests",
    "!build/tests_helpers"
  ],
  "exports": {
    ".": "./build/index.js",
    "./breadcrumbs_provider": "./build/providers/breadcrumbs_provider.js",
    "./breadcrumbs_middleware": "./build/src/breadcrumbs_middleware.js",
    "./services/registry": "./build/services/breadcrumbs_registry.js",
    "./types": "./build/src/types.js"
  },
  "scripts": {
    "build": "npm run compile",
    "clean": "del-cli build",
    "compile": "npm run clean && tsc",
    "format": "prettier --write .",
    "lint": "eslint . --ext=.ts",
    "precompile": "npm run lint",
    "prepublishOnly": "npm run build",
    "pretest": "npm run lint",
    "quick:test": "node --enable-source-maps --loader=ts-node/esm bin/test.ts",
    "release": "npx release-it",
    "test": "c8 npm run quick:test",
    "typecheck": "tsc --noEmit",
    "version": "npm run build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "@adonisjs/application": "^8.2.2",
    "@adonisjs/assembler": "^7.4.0",
    "@adonisjs/core": "^6.9.0",
    "@adonisjs/eslint-config": "^1.3.0",
    "@adonisjs/http-server": "^7.2.2",
    "@adonisjs/inertia": "1.0.0-26",
    "@adonisjs/lucid": "^20.5.1",
    "@adonisjs/prettier-config": "^1.3.0",
    "@adonisjs/route-model-binding": "^2.0.0-0",
    "@adonisjs/tsconfig": "^1.3.0",
    "@japa/assert": "^3.0.0",
    "@japa/plugin-adonisjs": "^3.0.1",
    "@japa/runner": "^3.1.2",
    "@poppinss/utils": "^6.7.3",
    "@swc/core": "^1.4.14",
    "@types/node": "^20.12.7",
    "@types/supertest": "^6.0.2",
    "better-sqlite3": "^9.6.0",
    "c8": "^9.1.0",
    "del-cli": "^5.1.0",
    "eslint": "^8.57.0",
    "pino-pretty": "^11.0.0",
    "prettier": "^3.2.5",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vitepress": "^1.1.4"
  },
  "peerDependencies": {
    "@adonisjs/core": "^6.9.0",
    "@adonisjs/lucid": "^20.5.1",
    "@adonisjs/route-model-binding": "^2.0.0-0",
    "luxon": "^3.4.4"
  },
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/app"
  },
  "prettier": "@adonisjs/prettier-config",
  "release-it": {
    "git": {
      "commitMessage": "chore(release): ${version}",
      "tagAnnotation": "v${version}",
      "tagName": "v${version}"
    },
    "hooks": {
      "before:init": [
        "npm test"
      ]
    },
    "github": {
      "release": true,
      "releaseName": "v${version}",
      "web": true
    }
  },
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "bin/**",
      "tests/**",
      "test_helpers/**"
    ]
  }
}
